@using IdentityService.Quickstart.UI
@model LogoutViewModel

<div class="logout-page">
    <div class="lead">
        <h1>Wylogowywanie</h1>
        <p>Czy jesteś pewny, że chcesz się wylogować?</p>
    </div>

    <form asp-action="Logout">
        <input type="hidden" name="logoutId" value="@Model.LogoutId"  />
        <div class="form-group">
            <button class="btn btn-primary">Tak</button>
        </div>
    </form>
</div>
